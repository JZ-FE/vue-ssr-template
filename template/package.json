{
  "name": "{{ name }}",
  "version": "1.0.0",
  "description": "{{ description }}",
  "author": "{{ author }}",
  "private": true,
  "scripts": {
    "dev": "node server",
    "start": "cross-env NODE_ENV=production node server",
    "build": "npm run build:client && npm run build:server && rimraf dist && node build/rename.js",
    "build:client": "cross-env NODE_ENV=production webpack --config build/webpack.client.config.js --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=production webpack --config build/webpack.server.config.js --progress --hide-modules",
    "build:single": "npm run build:client && rimraf dist && node build/rename.js",
    "add:component": "cd src/components && vue init billwing/vue-ssr-component .",
    "add:view": "cd src/views && vue init billwing/vue-ssr-view"{{#lint}},
    "lint": "cross-env NODE_ENV=production eslint --ext .js,.vue src"{{/lint}}
  },
  "engines": {
    "node": ">=6.0",
    "npm": ">=3.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "compression": "^1.7.2",
    "cookie-parser": "^1.4.3",
    "cross-env": "^5.1.4",
    "es6-promise": "^4.2.4",
    "express": "^4.16.3",
    "lodash": "^4.17.5",
    "lru-cache": "^4.1.2",
    "route-cache": "0.4.4",
    "serve-favicon": "^2.4.5",
    "vue": "^2.5.16",
    "vue-cookie": "^1.1.4",
    "vue-meta": "^1.5.0",
    "vue-router": "^3.0.1",
    "vue-server-renderer": "^2.5.16",
    "vuex": "^3.0.1",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "autoprefixer": "^8.2.0",
    "babel-core": "^6.26.0",
    {{#lint}}
    "babel-eslint": "^8.2.2",
    {{/lint}}
    "babel-loader": "^7.1.4",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-preset-env": "^1.6.1",
    "chokidar": "^2.0.3",
    "css-loader": "^0.28.11",
    {{#lint}}
    "eslint": "^4.19.1",
    "eslint-friendly-formatter": "^4.0.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-html": "^4.0.2",
    {{#if_eq lintConfig "standard"}}
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-promise": "^3.7.0",
    "eslint-plugin-standard": "^3.0.1",
    {{/if_eq}}
    {{#if_eq lintConfig "airbnb"}}
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-import-resolver-webpack": "^0.8.4",
    "eslint-plugin-import": "^2.9.0",
    {{/if_eq}}
    {{/lint}}
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-webpack-plugin": "^3.1.0",
    "node-sass": "^4.8.3",
    "postcss-loader": "^3.0.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^6.0.7",
    "sw-precache-webpack-plugin": "^0.11.5",
    "url-loader": "^1.0.1",
    "vue-loader": "^15.4.0",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^3.8.1",
    "webpack-bundle-analyzer": "^2.11.1",
    "webpack-dev-middleware": "^2.0.6",
    "webpack-hot-middleware": "^2.21.2",
    "webpack-merge": "^4.1.2",
    "webpack-node-externals": "^1.6.0"
  }
